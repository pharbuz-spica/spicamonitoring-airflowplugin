{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "units.json",
    "title": "Units in UI",
    "type": "string",
    "enum": [
        "NanoSecond",
        "MicroSecond",
        "MilliSecond",
        "Second",
        "Byte",
        "KiloByte",
        "KibiByte",
        "MegaByte",
        "MebiByte",
        "BytePerSecond",
        "BytePerMinute",
        "KiloBytePerSecond",
        "KiloBytePerMinute",
        "KibiBytePerSecond",
        "KibiBytePerMinute",
        "MegaBytePerSecond",
        "MegaBytePerMinute",
        "MebiBytePerSecond",
        "MebiBytePerMinute",
        "Ratio",
        "Percent",
        "Promille",
        "Count",
        "PerSecond",
        "PerMinute",
        "Unspecified",
        "NotApplicable"
    ],
    "$$target": "Unit"
}